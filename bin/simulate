#!/usr/bin/env ruby

require 'ants'

# Entry point here
include Ants

simulation = Sims::Simulation.new
simulation.start
simulation.analyze

cs = simulation.analyzer.clusters
data = cs.map{|c| c.map{|i| i.data} }

class Array; def mode; sort_by {|i| grep(i).length }.last; end; end
puts "data: #{cs.map{|c| c.map{|i| i.data[:target]}.sort }}"
puts "mode: #{cs.map{|c| c.map{|i| i.data[:target]}.mode }}"

binding.pry

